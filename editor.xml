<application>
  <component name="EditorSettings">
    <option name="CARET_BLINKING_PERIOD" value="300" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfix\b.*" />
    <pattern case-sensitive="true" pattern="\bNOTE\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bOneToOne\b. *">
      <option name="FOREGROUND" value="c05839" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bOneToMany\b. *">
      <option name="FOREGROUND" value="c05839" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bManyToMany\b. *">
      <option name="FOREGROUND" value="c05839" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bManyToOne\b. *">
      <option name="FOREGROUND" value="c05839" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a74c0" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingOpacity" value="15" />
  </component>
</application>