<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Liberation Mono" />
    <option name="LINE_SPACING" value="1.3" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JSON" value="true" />
        <entry key="JavaScript" value="true" />
        <entry key="LESS" value="true" />
        <entry key="PHP" value="true" />
        <entry key="SASS" value="true" />
        <entry key="SCSS" value="true" />
        <entry key="TypeScript" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="snakeyaml" value="true" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="CARET_BLINKING_PERIOD" value="300" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" case-sensitive="true" pattern="NOTE.*" />
    <pattern icon="question" case-sensitive="true" pattern="ManyToOne.*" />
    <pattern icon="question" case-sensitive="true" pattern="OneToOne.*" />
    <pattern icon="question" case-sensitive="true" pattern="OneToMany.*" />
    <pattern icon="question" case-sensitive="true" pattern="ManyToMany.*" />
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingOpacity" value="15" />
  </component>
</application>